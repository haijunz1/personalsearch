<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="g1105.metaps.entity.Album_picture" table="album_picture">
		<composite-id name="albumPicturePK" class="g1105.metaps.key.Album_picturePK">
			<key-property name="albumId" column="albumId" type="java.lang.Integer"></key-property>
			<key-property name="pictureId" column="pictureId" type="java.lang.String"></key-property>
		</composite-id>
	</class>
</hibernate-mapping>